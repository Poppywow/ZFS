<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:local="clr-namespace:Monitor">
    <SolidColorBrush x:Key="MDDivider" Color="#1F000000" po:Freeze="True" />
    <SolidColorBrush x:Key="MDBody" Color="#DD000000" po:Freeze="True" />
    <SolidColorBrush x:Key="MDTextBoxBorder" Color="#89000000" po:Freeze="True" />
    <SolidColorBrush x:Key="MDColumnHeader" Color="#BC000000" po:Freeze="True" />
</ResourceDictionary>